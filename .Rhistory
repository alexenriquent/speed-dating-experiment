data <- read.csv("dataset.csv", header = TRUE, stringsAsFactors = FALSE)
attributes <- data[, 25:30]
male_attributes <- data[data$gender == 1, 25:30]
female_attributes <- data[data$gender == 0, 25:30]
corrplot(cor(attributes, use = "complete.obs"), method = "shade",
title = "Attribute Correlation",
order = "hclust", addrect = 2, mar = c(0, 0, 2, 0),
tl.col = "black", addCoef.col = "black")
corrplot(cor(male_attributes, use = "complete.obs"), method = "shade",
title = "Men's Attributes (Rated by Female Participants)",
order = "hclust", addrect = 2, mar = c(0, 0, 2, 0),
tl.col = "black", addCoef.col = "black")
corrplot(cor(female_attributes, use = "complete.obs"), method = "shade",
title = "Women's Attributes (Rated by Male Participants)",
order = "hclust", addrect = 2, mar = c(0, 0, 2, 0),
tl.col = "black", addCoef.col = "black")
correlation_diff <- max(abs(cor(male_attributes, use = "complete.obs") - cor(female_attributes, use = "complete.obs")))
correlation_diff
plot_correlation <- function(correlation, title) {
corrplot(correlation, method = "shade", title = title,
order = "hclust", addrect = 2, mar = c(0, 0, 2, 0),
tl.col = "black", addCoef.col = "black")
}
plot.correlation <- function(correlation, title) {
corrplot(correlation, method = "shade", title = title,
order = "hclust", addrect = 2, mar = c(0, 0, 2, 0),
tl.col = "black", addCoef.col = "black")
}
plot.correlation <- function(correlation, title) {
corrplot(correlation, method = "shade", title = title,
order = "hclust", addrect = 2, mar = c(0, 0, 2, 0),
tl.col = "black", addCoef.col = "black")
}
plot.correlation(cor(attributes, use = "complete.obs"), "Attribute Correlation")
plot.correlation(cor(male_attributes, use = "complete.obs"), "Men's Attributes (Rated by Female Participants)")
plot.correlation(cor(female_attributes, use = "complete.obs"), "Women's Attributes (Rated by Male Participants)")
data <- read.csv("dataset.csv", header = TRUE, stringsAsFactors = FALSE)
